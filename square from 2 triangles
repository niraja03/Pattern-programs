// pattern problem of 2 triangles to square
#include<iostream>
using namespace std;

int main()
{
    int n=5;
    for(int i=i; i<=n; i++)
    {
        for(int j=1; j<=i; j++)
        {
            cout<<"*  ";
        }
        for(int j =i; j<=n; j++)
        {
            cout<<"*  ";
            }
        cout<<"\n";
    }
}
